	"scripts": {
		"dev": "pnpm run -C docs dev",
		"build": "gulp -f scripts/build/index.ts",
		"docs:build": "pnpm run -C docs build",
		"test": "vitest",
		"lint:fix": "eslint . --fix",
		"lint": "eslint --fix app/**/*.{ts,tsx,vue}",

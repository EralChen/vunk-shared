		"postinstall": "pnpm stub",
		"publish": "gulp -f scripts/publish.ts",
		"docker:publish": "gulp -f scripts/docker/publish.ts",
		"cc": "gulp -f scripts/create/component/index.ts",
		"ccd": "gulp -f scripts/create/component-docs/index.ts",
